<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="reporter.testListener"></listener>
	</listeners>
  <test thread-count="5" name="Test">
    <classes>
      <class name="PreRequisites.beforeSuiteTest"/>
      <class name="APITest.badgeID_positive"/>
      <class name="APITest.badgeID_negative"/>
      <class name="APITest.badgeTags_positive"/>
      <class name="APITest.badgeTags_negative"/>
      <class name="APITest.badgeReceipent_positive"/>
      <class name="APITest.badgeReceipent_negative"/>
    </classes>
  </test>
</suite>
